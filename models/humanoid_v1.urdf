    </collision>
    <inertial>
      <mass value="0.15"/>
      <inertia ixx="9.6e-05" ixy="0" ixz="0" iyy="9.6e-05" iyz="0" izz="9.6e-05"/>
    </inertial>
  </link>

  <joint name="head_pitch" type="revolute">
    <parent link="torso"/>
    <child link="head"/>
    <origin xyz="0 0 0.11499999999999999" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.5235987755982988" upper="0.5235987755982988" effort="10" velocity="1.5707963267948966"/>
  </joint>

